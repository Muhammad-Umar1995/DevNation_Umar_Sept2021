    "from datetime import date\n",
    "\n",
    "def get_date(no_of_days):\n",
    "    today=date.today()\n",
    "    x=today.strftime(\"%d\")\n",
    "    if no_of_days % 2 != 0:\n",
    "        \n",
    "        return int(x)+no_of_days\n",
    "    else:\n",
    "        return int(x)-no_of_days\n",
    "        \n",
    "print(get_date(6))\n"
